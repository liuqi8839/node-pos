<%- include header %>
<div class="col-xs-12 col-md-12 subAttribute">
    <div>
        <a  href="/admin">商品信息管理页</a>
        >>
        <a href="/goodsInfo/?name=<%= good.name%>"><%= good.name%></a>
        >>
        <label>删除属性</label>
    </div>
    <table class="table table-hover table-responsive">
        <tr>
            <td>属性名称</td>
            <td>属性值</td>
            <td>操作</td>
        </tr>
        <% for(var attributes in good){%>
        <% if(attributes == 'other') {%>
        <% good[attributes].forEach(function(attr){ %>
        <tr class="newAttr">
            <div class="col-md-4"><td><%= attr.attrName %></td></div>
            <div class="col-md-4"><td><%= attr.attrValue %></td></div>
            <div class="col-md-4">
                <td>
                    <a class="delete" href="#">删除</a>
                </td>
            </div>
        <% }) %>
        <% }}%>
        </tr>
    </table>
</div>

<script src="../viewStage/backstage/subAttribute.js"></script>

<%- include footer%>